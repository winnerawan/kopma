<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.ac.dharma_iswara.koperasimahasiswa"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="24" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="id.ac.dharma_iswara.koperasimahasiswa.config.AppController"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="id.ac.dharma_iswara.koperasimahasiswa.activity.SplashActivity"
            android:theme="@style/ThemeSplash" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="id.ac.dharma_iswara.koperasimahasiswa.activity.LoginActivity"
            android:theme="@style/ThemeSplash" />
        <activity
            android:name="id.ac.dharma_iswara.koperasimahasiswa.activity.MainActivity"
            android:theme="@style/ThemeSplash" />
    </application>

</manifest>